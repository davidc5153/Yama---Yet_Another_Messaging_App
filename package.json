        "@smui-extra/badge": "^6.0.0-beta.16",
        "@smui/button": "^6.0.0-beta.15",
        "@smui/checkbox": "^6.0.0-beta.16",
        "@smui/circular-progress": "^6.0.0-beta.16",
        "@smui/drawer": "^6.0.0-beta.15",
        "@smui/fab": "^6.0.0-beta.15",
        "@smui/form-field": "^6.0.0-beta.16",
        "@smui/icon-button": "^6.0.0-beta.15",
