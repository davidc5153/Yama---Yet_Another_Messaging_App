        "@smui/textfield": "^6.0.0-beta.15",
        "@smui/tooltip": "^6.0.0-beta.16",
        "@smui/top-app-bar": "^6.0.0-beta.15",
        "@tsconfig/svelte": "^2.0.0",
        "@types/crypto-js": "^4.1.1",
        "@types/node": "^17.0.24",
        "@types/webcrypto": "^0.0.28",
        "autoprefixer": "^10.4.4",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.12",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
