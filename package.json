    "dependencies": {
        "@dicebear/avatars": "^4.10.2",
        "@dicebear/avatars-identicon-sprites": "^4.10.2",
        "@material/button": "^13.0.0",
        "recordrtc": "^5.6.2",
        "sirv-cli": "^2.0.0",
